{
  "name": "fulfillment/oms-api",
  "description": "A wrapper for Fulfillment's OMS API",
  "keywords": [
    "fulfillment",
    "fulfillment.com"
  ],
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/Fufillment/oms-api-php",
  "authors":[
    {
      "name": "Matt Duncan"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "~6.0",
    "respect/validation": "1.1.*",
    "vlucas/phpdotenv": "~1.0",
    "league/climate": "^3.2",
    "netresearch/jsonmapper": "^0.8.0",
    "fulfillment/api-wrapper": "dev-master",
    "league/container": "^2.0@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Fulfillment\\OMS\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fulfillment\\OMS\\Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
